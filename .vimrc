set autoindent                                                                                                                                                      
set number
set nowrap
set tabstop=4
set shiftwidth=4
set incsearch
set ignorecase
set cursorline
noremap <S-j> gT
noremap <S-k> gt
filetype plugin indent on
nmap <F9> :NERDTreeToggle <CR>
let g:vim_markdown_folding_disabled=1
